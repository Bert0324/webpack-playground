{
  "compilerOptions": {
      "target": "es5",
      "module": "commonjs",
      "moduleResolution": "node",
      "declaration": true,
      "esModuleInterop": true,
      "resolveJsonModule": true,
      "experimentalDecorators": true,
      "sourceMap": true,
      "baseUrl": "./",
      "skipLibCheck": true,
      "noUnusedLocals": false,
      "noUnusedParameters": false
  }
}
